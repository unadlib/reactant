{
  "compilerOptions": {
    "outDir": "./dist",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "ES2019",
      "DOM"
    ],
    "jsx": "react",
    "declaration": true,
    "sourceMap": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,
    "forceConsistentCasingInFileNames": true
  }
}
